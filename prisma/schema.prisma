generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Admins {
  id        String   @id @default(uuid())
  v         Int      @map("__v") @default(0)
  createdAt DateTime @default(now())
  username  String
  email     String   @unique
  password  String
  updatedAt DateTime @updatedAt
}
